{
  "name": "react-startpack",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:kunukn/react-startpack.git",
  "author": "kunukn",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --env.dev --progress --config webpack.config.js",
    "build": "webpack --config webpack.config.build.js",
    "clean": "rimraf dist/*",
    "minify": "uglifyjs dist/myLibrary.umd.js --config-file uglify.config.json --output dist/myLibrary.umd.min.js"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": "last 2 versions, ie 11",
  "babel": {
    "presets": [
      "env",
      "stage-2",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-preset-env": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-2": "*",
    "cross-env": "*",
    "css-loader": "*",
    "extract-text-webpack-plugin": "*",
    "html-webpack-plugin": "*",
    "node-sass": "*",
    "parallel-webpack": "^2.2.0",
    "react": "*",
    "react-dom": "*",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "dependencies": {}
}
